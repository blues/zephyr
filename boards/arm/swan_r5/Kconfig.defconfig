if BOARD_SWAN_R5

config BOARD
	default "swan_r5"

config SWAN_STM32
  default y
  bool
  select USE_STM32_HAL_GPIO
  select USE_STM32_LL_GPIO

config SPI_STM32_INTERRUPT
	default y
	depends on SPI

if NETWORKING

config USB_DEVICE_STACK
	default y

config USB_DEVICE_NETWORK_EEM
	default y

endif # NETWORKING

endif # BOARD_SWAN_R5
